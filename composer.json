{
    "name": "wbcomdesigns/dokan-customer-management-enhanced",
    "description": "Enhanced customer management for Dokan vendors with LearnDash integration",
    "type": "wordpress-plugin",
    "keywords": ["wordpress", "dokan", "learndash", "customer-management", "woocommerce"],
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Wbcom Designs",
            "email": "admin@wbcomdesigns.com",
            "homepage": "https://wbcomdesigns.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "WbcomDesigns\\DokanCustomersEnhanced\\": "includes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WbcomDesigns\\DokanCustomersEnhanced\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs-check": "phpcs --standard=WordPress includes/",
        "cs-fix": "phpcbf --standard=WordPress includes/"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}